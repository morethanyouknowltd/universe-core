{
  "name": "@one/config",
  "version": "0.0.1",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "typescript": "4.8.4"
  },
  "main": "dist/cjs/index.js",
  "module": "index.js",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json",
    "./cli/*": {
      "import": "./cli/*.js",
      "require": "./dist/cjs/cli/*.js"
    },
    "./cli": {
      "import": "./cli/index.js",
      "require": "./dist/cjs/cli/index.js"
    },
    "./createHardConfigs": {
      "import": "./createHardConfigs.js",
      "require": "./dist/cjs/createHardConfigs.js"
    },
    "./index": {
      "import": "./index.js",
      "require": "./dist/cjs/index.js"
    },
    "./postbuild": {
      "import": "./postbuild.js",
      "require": "./dist/cjs/postbuild.js"
    },
    "./tryLoadConfig": {
      "import": "./tryLoadConfig.js",
      "require": "./dist/cjs/tryLoadConfig.js"
    }
  },
  "dependencies": {},
  "index": "dist/cjs/index.js"
}
